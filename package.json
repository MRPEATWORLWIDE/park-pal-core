{
  "private": true,
  "scripts": {
    "predev": "env $(awk 'NF > 0 && !/^#/ { print $0 }' .env.local | xargs) node npm-prebuild.js",
    "dev": "next dev",
    "prebuild": "node npm-prebuild.js",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "preexport": "npm run build",
    "export": "next export",
    "serve": "env $(awk 'NF > 0 && !/^#/ { print $0 }' .env.local | xargs) npm run export -o out/demo-store/ && npx serve out",
    "lint": "next lint",
    "ts:check": "tsc --noEmit && npm run test:data",
    "test:data": "jest ./data",
    "test": "jest --testPathIgnorePatterns ./data",
    "coverage": "jest --coverage",
    "dep:upgrade:major": "npx npm-check-updates -u",
    "dep:upgrade:minor": "npm run dep:upgrade:major -- --target minor",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/react-components": "^3.14.0",
    "@commercelayer/sdk": "^4.14.0",
    "@svgr/webpack": "^6.2.1",
    "@types/lodash": "^4.14.182",
    "fuse.js": "^6.6.2",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "next": "^12.1.6",
    "next-localization": "^0.12.0",
    "react": "^17",
    "react-dom": "^17",
    "redis": "^4.1.0",
    "swiper": "^8.2.4",
    "use-immer": "^0.7.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16",
    "@types/react": "^17",
    "autoprefixer": "^10.4.7",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "mock-fs": "^5.1.2",
    "next-sitemap": "^3.1.7",
    "postcss": "^8.4.14",
    "sass": "^1.53.0",
    "tailwindcss": "^3.1.4",
    "ts-jest": "^28.0.5",
    "typescript": "4.7.4"
  }
}
