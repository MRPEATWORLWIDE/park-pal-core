{
  "name": "@commercelayer/demo-store-website",
  "version": "2.0.0",
  "description": "This Demo Store is a completely static ecommerce solution (with SSR capability) powered by Commerce Layer.",
  "private": true,
  "scripts": {
    "sync:organization": "dotenv -c local node npm-sync-organization.js",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "preexport": "npm run build",
    "export": "next export",
    "dev": "next dev",
    "local:start": "dotenv -c local npm run build && npm start",
    "local:export": "dotenv -c local npm run export",
    "local:serve": "npm run local:export && npx serve out",
    "lint": "next lint",
    "ts:check": "tsc --noEmit && npm run test:data",
    "test:data": "jest ./data",
    "test": "jest --testPathIgnorePatterns ./data",
    "coverage": "jest --coverage",
    "lighthouse": "npx -p @lhci/cli lhci autorun"
  },
  "dependencies": {
    "@commercelayer/demo-store-types": "^2.0.0",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/react-components": "4.1.0",
    "@commercelayer/sdk": "^4.21.0",
    "@svgr/webpack": "^6.5.1",
    "@types/lodash": "^4.14.191",
    "fuse.js": "^6.6.2",
    "iframe-resizer-react": "^1.1.0",
    "immer": "^9.0.18",
    "lodash": "^4.17.21",
    "next": "^13.1.3",
    "next-localization": "^0.12.0",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-remark": "^2.1.0",
    "swiper": "^8.4.6",
    "use-immer": "^0.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13",
    "@types/node": "^18",
    "@types/react": "^18",
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^7.0.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "next-sitemap": "^3.1.45",
    "postcss": "^8.4.21",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": "^16.13.0 || ^18.0.0"
  }
}
