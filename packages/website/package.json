{
  "name": "@commercelayer/demo-store",
  "private": true,
  "scripts": {
    "prebuild": "dotenv -c local node npm-prebuild.js",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "preexport": "npm run build",
    "export": "next export",
    "dev": "npm run prebuild && next dev",
    "local:start": "dotenv -c local npm run build && npm start",
    "local:serve": "dotenv -c local npm run export -- -- -o out/demo-store/ && npx serve out",
    "lint": "next lint",
    "ts:check": "tsc --noEmit && npm run test:data",
    "test:data": "jest ./data",
    "test": "jest --testPathIgnorePatterns ./data",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@commercelayer/demo-store-types": "^0.0.0",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/react-components": "^3.14.1",
    "@commercelayer/sdk": "^4.15.1",
    "@svgr/webpack": "^6.3.1",
    "@types/lodash": "^4.14.182",
    "fuse.js": "^6.6.2",
    "iframe-resizer-react": "^1.1.0",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "next": "^12.2.3",
    "next-localization": "^0.12.0",
    "react": "^17",
    "react-dom": "^17",
    "react-remark": "^2.1.0",
    "redis": "^4.2.0",
    "swiper": "^8.3.2",
    "use-immer": "^0.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12",
    "@types/node": "^16",
    "@types/react": "^17",
    "autoprefixer": "^10.4.8",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "next-sitemap": "^3.1.15",
    "postcss": "^8.4.14",
    "sass": "^1.54.0",
    "tailwindcss": "^3.1.7",
    "ts-jest": "^28.0.7",
    "typescript": "4.7.4"
  }
}
