{
  "name": "@commercelayer/demo-store-website",
  "version": "1.0.0",
  "description": "This Demo Store is a completely static ecommerce solution (with SSR capability) powered by Commerce Layer.",
  "private": true,
  "scripts": {
    "sync:organization": "dotenv -c local node npm-sync-organization.js",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "preexport": "npm run build",
    "export": "next export",
    "dev": "next dev",
    "local:start": "dotenv -c local npm run build && npm start",
    "local:export": "dotenv -c local npm run export",
    "local:serve": "npm run local:export && npx serve out",
    "lint": "next lint",
    "ts:check": "tsc --noEmit && npm run test:data",
    "test:data": "jest ./data",
    "test": "jest --testPathIgnorePatterns ./data",
    "coverage": "jest --coverage",
    "lighthouse": "npx -p @lhci/cli lhci autorun"
  },
  "dependencies": {
    "@commercelayer/demo-store-types": "^1.0.0",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/react-components": "^3.14.1",
    "@commercelayer/sdk": "^4.15.1",
    "@svgr/webpack": "^6.3.1",
    "@types/lodash": "^4.14.184",
    "fuse.js": "^6.6.2",
    "iframe-resizer-react": "^1.1.0",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "next": "^12.2.5",
    "next-localization": "^0.12.0",
    "next-pwa": "^5.6.0",
    "react": "^17",
    "react-dom": "^17",
    "react-remark": "^2.1.0",
    "swiper": "^8.3.2",
    "use-immer": "^0.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12",
    "@types/node": "^16",
    "@types/react": "^17",
    "autoprefixer": "^10.4.8",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "next-sitemap": "^3.1.21",
    "postcss": "^8.4.16",
    "sass": "^1.54.5",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^28.0.8",
    "typescript": "4.7.4"
  }
}
